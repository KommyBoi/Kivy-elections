<GrNo>:
    name: "GrNo"
    BoxLayout:
        orientation: 'vertical'
        AnchorLayout:
            anchor_x: 'center'
            anchor_y: 'bottom'
            Label:
                text: 'Enter your GR Number :'
                font_size: 50
        AnchorLayout:
            anchor_x: 'center'
            anchor_y: 'top'
            TextInput:
                size_hint: (0.5, None)
                height: '45dp'  # Set an appropriate height
                width: '15dp' 
                multiline: False
                font_size: 35
                id: gr_input
        Button:
            text: "NEXT" 
            font_size: 32
            on_release: app.handle_button(gr_input)
            size_hint_y: (0.5)



<Heads>:
    canvas.before:
        Color:
            rgba: 0, 0.4, 0.4, 1  # Equivalent to hex #006671
        Rectangle:
            size: self.size
            pos: self.pos
    name: "Heads"
    BoxLayout:
        orientation: "vertical"
        Label:
            text: "ELECTIONS 2024-25" 
            font_size: 55

        Widget:
            size_hint_y: 0.4
        Label:
            text: "HEADBOY"
            font_size: 50
        
        BoxLayout:
            padding: 20
            spacing: 15 
            orientation: "horizontal"
            Widget:
                size_hint_x: 0.1
                size_hint_y: 0.1
            ToggleButton:
                size_hint: (None, None)
                width: 340
                height: 325
                group: 'HB'
                id: HB-1
                background_normal: "images/Rishit P-1.jpg"
                background_down: "images/Rishit P-2.jpg"
                font_size: 30
                on_state: app.options.HandleOptionB('HB-1', self.state)
            Widget: 
                size_hint_x: 0.5
            ToggleButton:
                width: 340
                height: 325
                size_hint: (None, None)
                group: 'HB'  # Group for headgirl options
                id: HB-2
                background_normal: "images/Shlok More -1.jpg" 
                background_down: "images/Shlok More -2.jpg" 
                font_size: 30
                valign: 'bottom'
                halign: 'center'
                on_state: app.options.HandleOptionB('HB-2', self.state)
                padding_x: 50
            Widget:
                size_hint_x: 0.1
                size_hint_y: 0.1
        Widget:
            size_hint_y: 1
            size_hint_x: 0.1
        Label: 
            text: "HEADGIRL"
            font_size: 50
        BoxLayout:
            orientation: "horizontal"
            Widget:
                size_hint_x: 0.1
                size_hint_y: 0.1
            ToggleButton:
                size_hint: (None,None)
                width: 340
                height: 300
                group: 'HG'  # Group for headgirl options
                id: HG-1
                background_normal: "images/Shruti G-1.jpg"
                background_down: "images/Shruti G-2.jpg"
                font_size: 30
                on_state: app.options.HandleOptionG('HG-2', self.state)
            Widget:
                size_hint_x: 0.4    
            ToggleButton:
                group: 'HG'  # Group for headgirl options
                id: HG-2
                size_hint: (None,None)
                width: 340
                height: 300
                background_normal: "images/Sanvi K-1.jpg"
                background_down: "images/Sanvi K-2.jpg" 
                on_state: app.options.HandleOptionG('HG-1', self.state)

            Widget:
                size_hint_x: 0.1
                size_hint_y: 0.1
        BoxLayout:
            Widget:
                size_hint_y: 0.1
                size_hint_x: 0.6
            Button:
                text: "NEXT"
                on_release: app.options.PopUpH(); app.database.voteUpdateB(); app.database.voteUpdateG()
                size_hint_y: (0.5)
            Widget:
